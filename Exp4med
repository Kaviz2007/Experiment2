#include<stdio.h>
void main()
{
    int a[5][5],i,j,m,n;
    int flag;
printf("enter rows and coloums:");
scanf("%d,%d",&m,&n);
for(i=0; i<m; i++)
    for(j=0; j<n; j++)
    {
        printf("enter a[%d][%d]",i,j);
        scanf("%d",&a[i][j]);
    }
for(i=0; i<m; i++)
    for(j=0; j<n; j++)
    {
        if(i==j&&a[i][j]!=1)
           flag=1;
        else {
            if(i==j&&a[i][j]!=1)
                flag=1;
        }
        if(flag==0)
            printf("\nthe given matrix is identify");
        else
            printf("\nnot identify matrix");
    }
    }
